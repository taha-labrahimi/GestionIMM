@model GestionIMM.Models.Propriete

@{
    ViewData["Title"] = "Edit";
}

<!DOCTYPE html>
<html>
<head>
	<meta charset="utf-8">
	<title>RegistrationForm_v6 by Colorlib</title>
	<meta name="viewport" content="width=device-width, initial-scale=1.0">

	<!-- MATERIAL DESIGN ICONIC FONT -->
	<link rel="stylesheet" href="~/FM/fonts/material-design-iconic-font/css/material-design-iconic-font.min.css">

	<!-- STYLE CSS -->
	<link rel="stylesheet" href="~/FM/css/style.css">
</head>

<body>

	<div class="wrapper">
		<div class="inner">
			<div class="image-holder">
				<img src="~/FM/images/bg12.jpg" alt="">
			</div>
			<form method="post" enctype="multipart/form-data">
				<div asp-validation-summary="ModelOnly" class="text-danger"></div>

				<!-- Row for Nom and Prenom -->
				<div class="form-row">
					<input asp-for="Type" class="form-control" placeholder="Propriete Name" />
					<span asp-validation-for="Type" class="text-danger"></span>
					<input asp-for="Taille" class="form-control" placeholder="Propriete Size" />
					<span asp-validation-for="Taille" class="text-danger"></span>
				</div> <br /><br />

				<!-- Other form fields -->
				<div class="form-row">
					<input asp-for="Emplacement" class="form-control" placeholder="Adresse" />
					<span asp-validation-for="Emplacement" class="text-danger"></span>
					<input asp-for="Caracteristiques" class="form-control" placeholder="Characteristic" />
					<span asp-validation-for="Caracteristiques" class="text-danger"></span>
				</div>
				<br /><br />
				<div class="form-row">
					<div class="form-group col-md-6">
						<input asp-for="Image" type="file" accept="image/*" name="uploadimage" id="uploadimage" />
					</div>
				</div>
				<br /><br />
				<!-- Other form fields -->
				<!-- ... (rest of your form fields) -->

				<button style="background:  #161A30; color: white;">
					Modify
					<i class="zmdi zmdi-long-arrow-right"></i>
				</button>
			</form>


		</div>
	</div>

	<script src="~/FM/js/jquery-3.3.1.min.js"></script>
	<script src="~/FM/js/main.js"></script>
</body><!-- This templates was made by Colorlib (https://colorlib.com) -->
</html>
